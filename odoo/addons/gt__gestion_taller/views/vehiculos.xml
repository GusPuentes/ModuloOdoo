<odoo>
  <data>
          <!--List view -->
    <record model="ir.ui.view" id="gt__gestion_taller.vehiculos_list" >
      <field name="name">Vehículos</field>
      <field name="model">gt__gestion_taller.vehiculos</field>
      <field name="arch" type="xml">
        <tree>
          <field name = "marca"/>
          <field name = "modelo"/>
          <field name="name" string="Matrícula"/>
        </tree>
      </field>  
    </record>

          <!--Form view -->

    <record model="ir.ui.view" id="gt__gestion_taller.vechiulos_form">
      <field name="name">Vehiculos</field>
      <field name="model">gt__gestion_taller.vehiculos</field>
      <field name="arch" type="xml">
        <form>
          <group col='4' colspan='1'>
            <group >
              <field name="marca"/>
              <field name="modelo"/>
              <field name="name" string="Matrícula"/>
            </group>
          </group>
        </form>
      </field>
    </record>      
  </data>
</odoo>